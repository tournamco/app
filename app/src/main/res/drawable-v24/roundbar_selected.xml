<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <stroke
                android:width="2dp"
                android:color="#000" />
        </shape>
    </item>

    <item
        android:bottom="1px"
        android:left="0dp"
        android:right="0dp">

        <shape android:shape="rectangle" >
            <solid android:color="@color/white" />



        </shape>
    </item>






</layer-list>